{
    "extends": "airbnb",
    "env": {
        "es6": true
    },
    "rules": {
        "indent": ["error", 4],
        "no-underscore-dangle": 0,
        "class-methods-use-this": 0,
        "guard-for-in": 0,
        "no-restricted-syntax": 0,
        "no-extra-boolean-cast": 0,
        "no-continue": 0,
        "arrow-body-style": 0,
        "new-cap": 0,
        "max-len": ["error", 160]
    }
}